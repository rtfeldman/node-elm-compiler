  "dependencies": {
    "cross-spawn": "6.0.5",
    "find-elm-dependencies": "2.0.2",
    "lodash": "4.17.20",
    "temp": "^0.9.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "chai": "4.2.0",
